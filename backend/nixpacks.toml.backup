# Backup of nixpacks.toml
# If Railway build fails, try deleting nixpacks.toml and let Railway auto-detect

[phases.setup]
nixPkgs = ["python310", "postgresql"]

[phases.install]
cmds = ["python -m pip install --upgrade pip", "python -m pip install -r requirements.txt"]

[phases.build]
cmds = ["echo 'Build phase complete'"]

[start]
cmd = "uvicorn main:app --host 0.0.0.0 --port $PORT"

